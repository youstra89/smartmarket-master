{% extends 'base.html.twig' %}


{% block title %}Modification de marque - {{ mark.label }}{% endblock %}


{% block notifications %}{% endblock %}


{% block body %}
  <div class="fh5co-narrow-content">
    <a href="javascript:history.back()" class="btn btn-primary">Retour</a><br><br>
    <h2 class="nimate-box" data-animate-effect="fadeInLeft">Modification de la marque <strong>{{ mark.label }}</strong> </h2>
    {{ include('Admin/Mark/_mark.html.twig', {button: 'Editer'}) }}
  </div>
{% endblock %}


{% block javascript %}
  <script>
    function submitForm(btn) {
        // disable the button
        btn.disabled = true;
        // submit the form    
        btn.form.submit();
    }
  </script>
{% endblock %}